.match-content-part{
  height: 410px;
  margin-left: 12px;
  margin-right: 12px;
}

.match-content-part>textarea{
  border: 1px solid #d4d4d4;
  height: calc(100% - 25px);
  width: calc(100% - 16px);
  border-radius: 2px;
  padding: 8px;
}

.text-info-part{
  margin-left: 12px;
  margin-bottom: 6px;
}

.text-info-part>text{
  margin-right: 24px;
}

.fmt-infos{
  font-weight: bold;
}

.list-chose{
  display: flex;
  justify-content: flex-start;
  margin-bottom: 10px;
  border-radius: 5px;
  margin-left: 12px;
}

.fmt-picker-part{
  border: 1px solid #d4d4d4;
  height: 32px;
  margin-right: 6px;
  border-radius: 2px;
  line-height: 32px;
  padding-left: 8px;
}



.made-single-part{
  margin-left: 12px;
  margin-bottom: 10px;
}

.fmt-operate-part{
  margin-left: 12px;
}


.fmt-btn{
  margin-left: 0px !important;
  font-weight: normal;
  background: #07c062;
  color: white;
  height: 32px;
  line-height: 32px;
  padding: 0px;
  width: 120px !important;
  display: inline-block;
  font-size: 16px;
}